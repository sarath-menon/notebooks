   "source": [
    "let\n",
    "\n",
    "x::Float64 = 64.0\n",
    "\n",
    "function myshow(x::Float64)\n",
    "    println(\"x= \" , x);\n",
    "end\n",
    "\n",
    "myshow(x) # custom  function \n",
    "\n",
    "@show x #macro\n",
    "\n",
    "# show generated code\n",
    "Base.remove_linenums!(@macroexpand @show x) \n",
    "end"
   ]
